{
	"manifest_version": 2,
	"name": "widehub",
	"description": "Make code wider",
	"version": "1.1",
	"content_scripts": [
		{
			"matches": ["https://github.com/*"],
			"exclude_matches": ["https://github.com/*blob*"],
			"js": ["jquery-2.1.1.js", "widehub_page.js"]
		},
		{
			"matches": ["https://github.com/*blob*"],
			"js": ["jquery-2.1.1.js", "widehub_blob.js"]
		},
		{
			"matches": ["https://github.com/*commit*/*diff=split"],
			"js": ["jquery-2.1.1.js", "widehub_commit_split_diff.js"]
		},
		{
			"matches": ["https://github.com/*commit*/*diff=unified"],
			"js": ["jquery-2.1.1.js", "widehub_commit_unified_diff.js"]
		}
	]	
}
